{
  "name": "lucky-snap-backend",
  "version": "0.0.1",
  "description": "Backend for Lucky Snap",
  "private": true,
  "scripts": {
    "build": "npx prisma generate && nest build",
    "start": "node index.js",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "postinstall": "npx prisma generate",
    "postbuild": "npm run generate",
    "generate": "npx prisma generate",
    "migrate:status": "npx prisma migrate status",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:data": "node migrate-data.js",
    "test:db": "node test-database.js",
    "backup": "node backup-data.js",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.17.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "prisma": "^5.22.0",
    "sqlite3": "^5.1.7",
    "ts-loader": "^9.4.3"
  }
}
