{
  "name": "lucky-snap",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm --prefix backend install && npm install",
    "dev": "concurrently -n FE,BE -c auto \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "npm --prefix backend run start:dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "npm --prefix backend run build",
    "preview": "vite preview",
    "typecheck": "npm --prefix backend run typecheck || true",
    "migrate:status": "npm --prefix backend run migrate:status",
    "migrate:deploy": "npm --prefix backend run migrate:deploy"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "framer-motion": "^12.23.15",
    "react-hook-form": "^7.62.0",
    "@prisma/client": "^6.16.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "terser": "^5.36.0",
    "concurrently": "^8.2.2"
  }
}
